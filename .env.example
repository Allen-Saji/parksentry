PORT=8080
NODE_ENV=development
LOG_LEVEL=info
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/parksentry
REDIS_URL=redis://localhost:6379
API_KEYS=dev-key-1,dev-key-2
# Detector mode: mock | http
DETECTOR_MODE=mock
# Required when DETECTOR_MODE=http
# DETECTOR_HTTP_ENDPOINT=http://localhost:8000/detect
DETECTOR_FALLBACK_MOCK=true
DETECTOR_SAMPLE_EVERY=6
DETECTOR_MAX_FRAMES=100
# Virtual gate line X-position (pixels) for entry/exit heuristic inference
GATE_LINE_X=160
# Optional role header for testing protected routes: x-roles=operator,worker,admin
